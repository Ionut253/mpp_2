{
  "name": "banking_manager_app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "seed": "ts-node --project tsconfig.seed.json prisma/seed.ts",
    "create:admin": "ts-node --project tsconfig.seed.json prisma/seed-admin.ts",
    "delete:admin": "ts-node --project tsconfig.seed.json prisma/delete-admin.ts",
    "reset:admin": "npm run delete:admin && npm run create:admin",
    "generate:test-data": "ts-node --project tsconfig.seed.json scripts/generate-test-data.ts",
    "test:performance": "ts-node --project tsconfig.seed.json scripts/test-statistics-performance.ts",
    "test:all": "npm run generate:test-data && npm run test:performance",
    "deploy": "node scripts/deploy.js",
    "deploy:prod": "set NODE_ENV=production&& node scripts/deploy.js"
  },
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "@sendgrid/mail": "^8.1.5",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/table-core": "^8.21.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/lodash": "^4.17.16",
    "@types/node": "20.11.20",
    "@types/node-fetch": "^2.6.12",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "18.2.58",
    "@types/react-dom": "18.2.19",
    "@upstash/redis": "^1.28.4",
    "@vercel/kv": "^1.0.1",
    "autoprefixer": "10.4.17",
    "bcryptjs": "^3.0.2",
    "date-fns": "^4.1.0",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "express-rate-limit": "^7.5.0",
    "jose": "^6.0.11",
    "lodash": "^4.17.21",
    "lucide-react": "^0.510.0",
    "next": "14.1.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.1",
    "postcss": "8.4.35",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "prisma": "^6.8.2",
    "ts-node": "^10.9.2"
  },
  "prisma": {
    "seed": "ts-node --project tsconfig.seed.json prisma/seed.ts"
  }
}
